- if @projects.empty?
  %p None :(
- else
  %ul.projects
    - @projects.each do |project|
      %li.project{:id => dom_id(project), :name => project.name}
        %h2.name= link_to project.name, cijoe_path(project)
        %p.status= project.status


#metadata{:style => 'display:none'}
  %span.updated_at= Time.now.as_json
